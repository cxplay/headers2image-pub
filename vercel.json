{
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/json"
    },
    {
      "source": "/json",
      "destination": "/api/json"
    },
    {
      "source": "/json-(.*)",
      "destination": "/api/json"
    },
    {
      "source": "/json.json",
      "destination": "/api/json"
    },
    {
      "source": "/json-(.*).json",
      "destination": "/api/json"
    },
    {
      "source": "/image",
      "destination": "/api/image.png"
    },
    {
      "source": "/image-(.*)",
      "destination": "/api/image.png"
    },
    {
      "source": "/image.png",
      "destination": "/api/image.png"
    },
    {
      "source": "/image-(.*).png",
      "destination": "/api/image.png"
    }
  ]
}
